<h1>Listing users</h1>
 
<table>
  <tr>
    <th>first_name</th>
    <th>last_name</th>
    <th>email</th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.email %></td>
      <td><%= link_to 'Show', user_path(user) %></td>
      
    </tr>
  <% end %>
</table>


<%= link_to "New User", new_user_path %>

